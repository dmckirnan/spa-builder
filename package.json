{
  "name": "spa-builder",
  "version": "1.0.0",
  "description": "Scaffolding manager for SPA in React",
  "main": "index.js",
  "scripts": {
    "start": "npm run setup",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "nodemon ./server/server.js",
    "dev-client": "webpack-dev-server -d --hot --config webpack.config.js --watch",
    "setup": "concurrently --kill-others \"npm run build\" \"npm run server\"",
    "server": "nodemon server/server.js",
    "build": "webpack -p --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/config.js \"src/**/*.spec.js\"",
    "tdd": "npm test -- --watch",
    "postinstall": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmckirnan/spa-builder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dmckirnan/spa-builder/issues"
  },
  "homepage": "https://github.com/dmckirnan/spa-builder#readme",
  "dependencies": {
    "autoprefixer": "^8.5.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "eslint-import-resolver-webpack": "^0.9.0",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "sinon-chai": "^3.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.10.0",
    "karma": "^2.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.2",
    "mocha": "^5.1.1",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.2",
    "sinon": "^5.0.7",
    "webpack-dev-server": "^2.9.7"
  }
}
